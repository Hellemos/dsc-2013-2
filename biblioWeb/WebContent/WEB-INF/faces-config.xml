<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<managed-bean-name>livroMB</managed-bean-name>
		<managed-bean-class>biblio.mb.LivroMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>cadastro</display-name>
		<from-view-id>/cadastro.jsp</from-view-id>
		<navigation-case>
			<from-outcome>ok</from-outcome>
			<to-view-id>/principal.html</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
